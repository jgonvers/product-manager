<div class="container">
  <h1 class="text-center mb-5 mt-2">Articles client</h1>
  <% @client_articles.each do |article| %>
    <%= link_to article_path(article.article), class: "text-reset text-decoration-none" do %>
      <%= render  "client_articles/card_client_article", article: article %>
    <% end %>
  <% end %>
</div>